{
  "name": "@humandbs/backend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "types": "./types/shared-types.ts",
  "exports": {
    "./types": "./types/shared-types.ts"
  },
  "scripts": {
    "dev": "bun run --watch src/api/run-server.ts",
    "crawler:download-html": "bun run src/crawler/download-html.ts",
    "crawler:crawl": "bun run src/crawler/crawl.ts",
    "crawler:normalize": "bun run src/crawler/normalize.ts",
    "crawler:transform": "bun run src/crawler/transform.ts",
    "crawler:fetch-external-api": "bun run src/crawler/fetch-external-api.ts",
    "crawler:extract-fields": "bun run src/crawler/extract-fields.ts",
    "crawler:export-tsv": "bun run src/crawler/export-tsv.ts",
    "crawler:import-tsv": "bun run src/crawler/import-tsv.ts",
    "es:load-mappings": "node src/es/load-mappings.ts",
    "es:load-docs": "node src/es/load-docs.ts",
    "test": "bun test tests/unit",
    "test:watch": "bun test tests/unit --watch",
    "test:integration": "bun test tests/integration",
    "test:all": "bun test",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src tests",
    "lint:fix": "eslint src tests --fix"
  },
  "devDependencies": {
    "@humandbs/eslint-config": "workspace:*",
    "@types/bun": "latest",
    "@types/jsdom": "^21.1.7",
    "eslint": "^9.19.0"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^8.17.0",
    "@hono/node-server": "^1.18.1",
    "@hono/swagger-ui": "^0.5.2",
    "@hono/zod-openapi": "^1.1.0",
    "@stylistic/eslint-plugin": "^5.3.1",
    "@types/yargs": "^17.0.33",
    "hono": "^4.8.12",
    "jose": "^6.0.12",
    "jsdom": "^26.0.0",
    "yargs": "^17.7.2",
    "zod": "^4.0.15"
  }
}
