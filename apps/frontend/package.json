{
  "name": "@humandbs/frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite dev --port 3000 --host 0.0.0.0",
    "build": "vite build",
    "auth:generate": "bunx --bun @better-auth/cli@latest generate --config ./src/lib/auth.ts --y --output ./src/db/schema/auth-schema.ts && prettier --write ./src/db/schema/auth-schema.ts",
    "db:generate": "bunx --bun drizzle-kit generate",
    "db:push": "bunx --bun drizzle-kit push",
    "start": "bun ./.output/server/index.mjs",
    "get-md": "bun ./src/scripts/parsePageToMarkdown.js \"$@\"",
    "db:studio": "POSTGRES_HOST=localhost bunx drizzle-kit studio",
    "db:seed": "bun ./src/scripts/seed.ts",
    "db:reset": "bun ./src/scripts/reset-db.ts",
    "db:fresh": "bun run db:reset && bun run db:push && bun run db:seed"
  },
  "dependencies": {
    "@codemirror/lang-markdown": "^6.3.3",
    "@formatjs/intl-localematcher": "^0.6.1",
